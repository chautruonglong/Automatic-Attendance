        <StackLayout>
            <Picker IsEnabled="True" IsVisible="True" x:Name="PickerSubject" >
                <Picker.Items>
                    <x:String>Khoa học dữ liệu</x:String>
                    <x:String>Vi điều khiển</x:String>
                    <x:String>Xử lí tín hiệu số</x:String>
                </Picker.Items>
            </Picker>

            <Picker IsEnabled="True" IsVisible="True" x:Name="PickerClass" >
                <Picker.Items>
                    <x:String>18Nh13</x:String>
                    <x:String>18Nh14</x:String>
                    <x:String>18Nh15</x:String>
                </Picker.Items>
            </Picker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>